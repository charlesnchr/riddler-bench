{
  "version": 2,
  "builds": [
    {
      "src": "results-ui/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "results-ui/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/results-ui/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/results-ui/build/index.html"
    }
  ],
  "functions": {
    "results-ui/server/index.js": {
      "includeFiles": "results/**"
    }
  },
  "env": {
    "RESULTS_DIR": "results",
    "NODE_ENV": "production"
  }
}